{
  "name": "Conflict",
  "author": "andofwinds",
  "dark": true,
  "editorScheme": "/editor.xml",
  "colors": {
    "bg": "#24294b",
    "bg-light": "#353d70",
    "bg-dim": "#1b1e36",
    "border": "#222444",
    "white": "#E2E2EC",
    "accent": "#DAB4C8",
    "accent-dim": "#9C8AB4",
    "text": "#BFC8D7",
    "text-dim": "#7e859b",
    "debug": "#FF00FF"
  },
  "ui": {
    "*": {
      "background": "bg",
      "foreground": "text",
      "borderColor": "border",
      "inactiveBackground": "bg-dim",
      "disabledBackground": "bg-dim",
      "selectionBackgroundInactive": "bg",
      "selectionInactiveBackground": "bg-light"
    },
    "ActionButton": {
      "hoverBackground": "bg-light",
      "pressedBackground": "bg-light",
      "hoverBorderColor": "bg",
      "pressedBorderColor": "border"
    },
    "CompletionPopup": {
      "selectionBackground": "bg-light",
      "matchForeground": "accent-dim",
      "foreground": "text"
    },
    "Button": {
      "foreground": "text",
      "startBackground": "bg-light",
      "endBackground": "bg-light",
      "startBorderColor": "bg",
      "endBorderColor": "bg",
      "shadowColor": "bg",
      "focusedBorderColor": "border",
      "default": {
        "foreground": "text",
        "startBackground": "bg-light",
        "endBackground": "bg-light",
        "startBorderColor": "accent-dim",
        "endBorderColor": "accent-dim",
        "shadowColor": "bg",
        "focusedBorderColor": "border"
      }
    },
    "EditorTabs": {
      "underlinedTabBackground": "bg-light",
      "hoverBackground": "bg-dim",
      "underlinedTabForeground": "text"
    },
    "Plugins": {
      "background": "bg",
      "disabledForeground": "text-dim",
      "hoverBackground": "bg-dim",
      "lightSelectionBackground": "bg-light"
    }
  }
}